@model IEnumerable<Category>

@if (Model.Count() > 0)
{
    <div class="list-group">
        <a asp-action="Index" asp-controller="Catalog" class="list-group-item list-group-item-action">Wszystkie</a>
        @foreach (var category in Model)
        {
        <a asp-action="Index" asp-controller="Catalog" asp-route-categoryId="@category.Id" class="list-group-item list-group-item-action">@category.Name</a>
        }
    </div>
}
else
{
    <p>Brak kategorii</p>
}